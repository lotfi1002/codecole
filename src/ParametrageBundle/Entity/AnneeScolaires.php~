?php

namespace ParametrageBundle\Entity;

/**
 * AnneeSolaires
 */
class AnneeScolaires
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var int
     */
    private $annee;

    /**
     * @var boolean
     */
    private $isActive;

    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set annee
     *
     * @param integer $annee
     *
     * @return AnneeSolaires
     */
    public function setAnnee($annee)
    {
        $this->annee = $annee;

        return $this;
    }

    /**
     * Get annee
     *
     * @return int
     */
    public function getAnnee()
    {
        return $this->annee;
    }
}
